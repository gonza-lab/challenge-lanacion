{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/jest",
    "module": "commonjs",
    "moduleResolution": "node10","types": ["jest", "node"],
    "resolveJsonModule": true,
		"esModuleInterop": true
  },
  "include": [
    "jest.config.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/**/*.d.ts",
		"src/**/*.json",
		"src/**/*.ts"
  ]
}
